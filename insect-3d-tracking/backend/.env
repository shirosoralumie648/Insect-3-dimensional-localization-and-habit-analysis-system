# 应用配置
APP_NAME=InsectTracker
APP_VERSION=1.0.0
DEBUG=True
SECRET_KEY=your_secret_key_here_change_in_production
API_PREFIX=/api

# 数据库配置
DATABASE_URL=sqlite:///./insect_tracker.db

# 视频和数据存储路径
STORAGE_PATH=./storage
VIDEO_PATH=./storage/videos
DATASET_PATH=./storage/datasets
MODEL_PATH=./storage/models

# 相机设置
DEFAULT_RESOLUTION=1280x720
DEFAULT_FPS=30

# YOLO设置
YOLO_MODEL_PATH=./storage/models/yolov8n.pt
CONFIDENCE_THRESHOLD=0.25
